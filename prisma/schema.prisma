generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model contact_inquiries {
  id           Int      @id @default(autoincrement())
  name         String
  organization String
  interest_type String   @default("General")
  email        String
  phone        String
  message      String
  created_at   DateTime @default(now())
}
